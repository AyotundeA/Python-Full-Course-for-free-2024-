# Lesson 25 - Logical Operators Part 3

num = 5
a = 6
b = 7
age = 13
temperature = 20
user_role = "guest"

type = "Positive" if num > 0 else "Negative"
result = "EVEN" if num % 2 == 0 else "ODD"
max_num = a if a > b else b
min_num = a if a < b else b
status = "Adult" if age >= 18 else "Child"
weather = "HOT" if temperature > 20 else "COLD"
access_level = "Full Access" if user_role == "admin" else "Limited Access"

print("Positive or Negative Number: " + type)
print("Even or Odd Number: " + result)
print("Access Level: " + access_level)
print("Child or Adult: " + status)
print("Hot or Cold: " + weather)
